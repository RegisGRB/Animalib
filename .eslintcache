ntForm = {\n  initial: { x: \"100vh\", transition },\n  show: { x: 0, transition },\n  exit: { x: \"-100vh\", transition },\n};\n\nconst FormContainer = styled(motion.div)`\n  position: absolute;\n`;\nconst SignTitle = styled(SpanText)`\n  margin-bottom: 10%;\n  font-size: 1.5rem;\n  color:${(props) => props.Error ? \"red\" : \"\"}\n`;\nconst SignLogo = styled.img`\n  width: 50%;\n  filter: brightness(0.5);\n`;\n\nconst StyledButtonSwitch = styled.span`\n  position: absolute;\n  bottom: 25%;\n  right: 30%;\n  color: ${(props) => props.theme.colors.third};\n  cursor: pointer;\n`;\n\nexport default AniForm;\n","/Users/wassim/Projects/Animalib/src/components/Motion/MotionModal.js",[],"/Users/wassim/Projects/Animalib/src/components/StyledComponents/StyledTheme.js",[],"/Users/wassim/Projects/Animalib/src/Hooks/useSize.js",["286"],"import { useState, useEffect } from \"react\";\n\n// Hook\nconst useWindowSize = () => {\n  const isClient = typeof window === \"object\";\n\n  const getSize = () => {\n    return {\n      width: isClient ? window.innerWidth : undefined,\n      height: isClient ? window.innerHeight : undefined\n    };\n  }\n\n  const [windowSize, setWindowSize] = useState(getSize);\n\n  useEffect(() => {\n    if (!isClient) {\n      return false;\n    }\n\n    const handleResize = () => {\n      setWindowSize(getSize());\n    }\n\n    window.addEventListener(\"resize\", handleResize);\n    return () => window.removeEventListener(\"resize\", handleResize);\n  }, []); \n\n  return windowSize;\n}\n\nexport default useWindowSize;","/Users/wassim/Projects/Animalib/src/components/elements/Input.jsx",[],"/Users/wassim/Projects/Animalib/src/Theme/Config.js",[],"/Users/wassim/Projects/Animalib/src/Hooks/useLocalStorage.js",[],"/Users/wassim/Projects/Animalib/src/components/Motion/MotionFade.js",[],"/Users/wassim/Projects/Animalib/src/components/Motion/MotionBox.js",[],"/Users/wassim/Projects/Animalib/src/utils/auth.js",["287"],"const auth = {\n    setUser: function (use