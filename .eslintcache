component: Pages.AddAnimal,\n      protectedRoute: true,\n      nav: {\n        show: false,\n        icon: <AiOutlineProfile />,\n        name: \"OverView\",\n      },\n    },\n    {\n      // AnimalPlate Edit de l'animal\n      path: \"/EditAnimal/:id\",\n      component: Pages.EditAnimal,\n      protectedRoute: true,\n      nav: {\n        show: false,\n        icon: <AiOutlineProfile />,\n        name: \"EditAnimal\",\n      },\n    },\n    {\n      // AnimalPlate Edit de l'animal\n      path: \"/Calendar\",\n      component: Pages.Calendar,\n      protectedRoute: true,\n      nav: {\n        show: false,\n        icon: <AiOutlineProfile />,\n        name: \"OverView\",\n      },\n    },\n  ];\n\n  return (\n    <BrowserRouter>\n      <Route\n        render={({ location }) => (\n          <>\n            {/* <NavBar routes={routes} location={location}></NavBar> */}\n            {/* Si besoin de navbar dynamic web et mobile*/}\n            <AnimatePresence initial={false} exitBeforeEnter={true}>\n              <Switch location={location} key={location.pathname}>\n                {routes.map((route) =>\n                  route.protectedRoute === true &&\n                  !localStorage.getItem(\"token\") ? (\n                    <Redirect to=\"/\"></Redirect>\n                  ) : (\n                    <Route\n                      exact\n                      key={route.path}\n                      path={route.path}\n                      component={route.component}\n                      protectedRoute={route.protectedRoute}\n                    />\n                  )\n                )}\n              </Switch>\n            </AnimatePresence>\n          </>\n        )}\n      />\n    </BrowserRouter>\n  );\n};\nexport default Router;\n","/Users/wassim/Projects/Animalib/src/Store/store.js",[],"/Users/wassim/Projects/Animalib/src/context/index.js",[],"/Users/wassim/Projects/Animalib/src/components/ui/NavBar/NavBar.js",["254","255","256","257","258","259"],"imp